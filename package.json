{
  "name": "termProject",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@3.6.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/app.ts",
    "build": "NODE_ENV=production tsc --project tsconfig.json --diagnostics  -p tsconfig.json",
    "dev:naver": "nodemon --exec node -r  ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/libraries/scrape/naver.kin.ts",
    "schema": "prisma generate",
    "migrate": "prisma migrate dev --name added_melon",
    "db:update": "prisma db push"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^18.16.0",
    "@types/node-schedule": "^2.1.0",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.1.3",
    "moment": "^2.29.4",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "set-interval-async": "^3.0.3",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
