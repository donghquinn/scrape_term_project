// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NaverKin {
  uuid String @id @default(uuid())
  title String
  category String
  content String
  link String
  create DateTime @default(now())
}